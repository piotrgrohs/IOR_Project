<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Usluga" table="USLUGA" schema="hiberlab">
        <id name="id" column="USLUGA_ID">
            <generator class="native"/>
        </id>
        <property name="cena" column="CENA"/>
        <property name="opis" column="OPIS"/>
        <property name="rabat" column="RABAT"/>
        <many-to-one name="wizyta" foreign-key="FK_USL_WIZ" unique="false" column="WIZYTA_ID" class="model.Wizyta" cascade="save-update"/>
        <many-to-one name="cennik" foreign-key="FK_USL_CEN" unique="false" column="CENNIK_ID" class="model.Cennik" cascade="save-update"/>
    </class>
</hibernate-mapping>