<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Adres" table="ADRESY">
        <id name="id" column="ADRES_ID"/>
        <property name="kod" column="KOD"/>
        <property name="miejscowosc" column="MIEJSCOWOSC"/>
        <property name="ulica" column="ULICA"/>
        <property name="nrDomu" column="NRDOMU"/>
        <property name="nrMieszkania" column="NRMIESZKANIA"/>
    </class>
</hibernate-mapping>