<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Wizyta" table="WIZYTY" >
        <id name="id" column="WIZYTA_ID">
            <generator class="native"/>
        </id>
        <property name="data" column="data"/>
        <property name="typ" column="typ"/>
        <many-to-one name="lekarz" foreign-key="FK_WIZ_LEK" unique="false" column="LEKARZ_ID" class="model.Lekarz" />
        <many-to-one name="pacjent" foreign-key="FK_WIZ_PAC" unique="false" column="PACJENT_ID" class="model.Pacjent" />
        <one-to-one name="cennik" class="model.Cennik" foreign-key="FK_WIZ_CEN"  cascade="save-update"/>
    </class>
</hibernate-mapping>