<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Zabieg" table="ZABIEG" schema="hiberlab">
        <id name="id" column="ZABIEG_ID">
            <generator class="increment"/>
        </id>
        <property name="nrZeba" column="NRZEBA"/>
        <property name="opis" column="OPIS"/>
        <one-to-one name="wizyta" class="model.Wizyta"  foreign-key="FK_ZAB_WIZ"  cascade="save-update"/>
    </class>
</hibernate-mapping>